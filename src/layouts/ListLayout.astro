---
import Layout from './Layout.astro';
const { content } = Astro.props;

---

<Layout title={content.title}>
    <div class="pb-5 ">
        <div class="text-right pr-3 pb-3">Update on: {content.date}</div>
        <div class="flex ">
            <slot />
        </div>
    </div>

    <!-- <List markdown={`#test`}/> -->
</Layout>

<style>
    div.flex {
        justify-content: center;
    }
</style>